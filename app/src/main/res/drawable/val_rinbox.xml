<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="-1dp"
        android:left="1dp"
        >
        <shape>

            <stroke
                android:width="1.5dp"
                android:dashGap="1dp"
                android:dashWidth="3dp"
                android:color="@color/black" />
            <size
                android:width="10dp"
                android:height="10dp" />
            <corners
                android:topLeftRadius="0.2dp"
                android:topRightRadius="5dp"
                android:bottomLeftRadius="0.2dp"
                android:bottomRightRadius="5dp"/>
            <solid
                android:color="@color/cFFFFFF" />
            <padding

                android:left="2dp"
                android:right="-1dp"
                android:top="2dp"
                android:bottom="-2dp"
                />
        </shape>
    </item>
</layer-list>